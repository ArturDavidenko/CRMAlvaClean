@{
}

@model List<Order>


<div>

    @if (Model.Count > 0)
    {
        @foreach (var item in Model)
        {
            <div>
                @item.OrderDate
                @item.Status
                @item.OrderPriceType

                Employeers count: @item.Employeers.Count 
                
            </div>
        }

    }
    else
    {
        <div>
            Orders not exist!
        </div>
    }


</div>
